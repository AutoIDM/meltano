{
    "$schema": "http://iglucentral.com/schemas/com.snowplowanalytics.self-desc/schema/jsonschema/1-0-0#",
    "description": "Schema for the general Meltano context",
    "self": {
        "vendor": "com.meltano",
        "name": "general_context",
        "format": "jsonschema",
        "version": "1-0-0"
    },
    "type": "object",
    "properties": {
        "project_id": {
            "type": "string"
        },
        "meltano_version": {
            "type": "string"
        },
        "python_version": {
            "type": "string"
        },
        "operating_system": {
            "type": "string"
        },
        "caller_process": {
            "type": ["string", "null"]
        },
        "context_uuid": {
            "type": "string"
        },
        "environment": {
            "type": ["string", "null"]
        }
    },
    "required": [
        "project_id",
        "meltano_version",
        "python_version",
        "operating_system",
        "context_uuid"
    ],
    "additionalProperties": false
}
